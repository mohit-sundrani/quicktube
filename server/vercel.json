{
    "version": 2,
    "buildCommand": "prisma generate && tsc",
    "installCommand": "yarn",
    "builds": [
        {
            "src": "src/index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/index.ts"
        }
    ]
}
